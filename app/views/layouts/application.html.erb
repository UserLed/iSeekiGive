  <%= stylesheet_link_tag "application", :media => "all" %>
  <!-- END Demo Examples -->
</head>
<body class="width1200">
<div class="container-global">
  <%= render "shared/demo_header" %>

  <%= bootstrap_flash %>
  <%= yield %>

  <!-- /wrap -->

  <%= render "shared/demo_footer" %>
</div>
<%= javascript_include_tag "bottom" %>


